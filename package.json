{
  "name": "pro-build-engine",
  "version": "2.0.0",
  "description": "React Application Webpack Build Engine",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run satisfied --fix",
    "source": "rimraf ./lib && babel src --out-dir lib -s",
    "cli": "babel cli/index.js > index.js",
    "build": "npm run source && npm run cli",
    "lint:engine": "eslint 'src/**/*.js' -f table --fix",
    "lint:test": "eslint -c specs/.eslintrc 'specs/**/*.spec.js' -f table --no-eslintrc",
    "test:watch": "jest --config=specs/jest-config.json --watch",
    "lint": "npm-run-all lint:*",
    "posttest": "npm run build",
    "prepublish": "npm run test",
    "test": "npm run lint",
    "doc": "jsdoc -c conf.json -R README.md",
    "check": "npm-check -u",
    "satisfied": "satisfied --skip-invalid"
  },
  "keywords": [
    "React",
    "Webpack",
    "Boilerplate",
    "Application",
    "Build",
    "Engine"
  ],
  "precommit": [
    "test",
    "build"
  ],
  "author": {
    "name": "SAPIENT",
    "email": "skumar146@sapient.com"
  },
  "license": "ISC",
  "engines": {
    "node": ">= 6",
    "npm": ">= 4"
  },
  "dependencies": {
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-sxt-engine": "^1.0.1",
    "babel-watch": "^2.0.7",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.15.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "cross-spawn": "^6.0.5",
    "crypto-js": "^3.1.9-1",
    "csp-html-webpack-plugin": "^2.3.0",
    "css-loader": "^0.28.11",
    "eslint-config-sxt-engine": "^1.0.9",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^23.1.0",
    "jsdoc": "^3.5.5",
    "json-server": "^0.14.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-check": "^5.7.1",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.5",
    "regenerator-runtime": "^0.11.1",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "satisfied": "^1.1.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-formatter-table": "^1.0.4",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-command": "^0.2.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "mock-fs": "^4.5.0"
  },
  "bin": {
    "engine": "./index.js"
  }
}
